{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "VictoriaMetrics/victoria-logs-single",
      "extractVersionTemplate": "^victoria-logs-single-(?<version>.+)$",
      "fileMatch": [
        "roles/victorialogs/Chart.yaml",
        "roles/victorialogs/default/main.yaml"
      ],
      "matchStrings": [
        "(?:^version:\\s+(?<currentValue>.+))",
        "helm:\\s+chart:[\\s\\S]+?version:\\s+v(?<currentValue>.+)"
      ],
      "packageNameTemplate": "VictoriaMetrics/helm-charts"
    }
  ],
  "enabledManagers": [
    "custom.regex"
  ],
  "extends": [
    "config:recommended"
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
